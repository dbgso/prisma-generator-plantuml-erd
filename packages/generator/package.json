{
  "name": "prisma-generator-plantuml-erd",
  "description": "Provide a description about your generator",
  "version": "1.7.1",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-plantuml-erd": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "pnpm exec tsc -w",
    "build": "pnpm exec tsc",
    "prepack": "pnpm build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "5.19.0",
    "@prisma/generator-helper": "5.19.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@prisma/internals": "5.19.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "29.2.2",
    "@types/node": "22.5.1",
    "@types/prettier": "2.4.2",
    "jest": "29.3.0",
    "prettier": "2.7.1",
    "prisma": "5.19.0",
    "semantic-release": "24.1.0",
    "ts-jest": "29.0.3",
    "typescript": "4.9.5"
  },
  "homepage": "https://github.com/dbgso/prisma-generator-plantuml-erd/tree/main/packages/generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbgso/prisma-generator-plantuml-erd.git"
  },
  "author": "osaki.shin <osaki.shin@dbgso.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "plantuml"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "node -e \"const pkg=require('./package.json');pkg.version='${nextRelease.version}';require('fs').writeFileSync('./package.json',JSON.stringify(pkg,null,2)+'\\n')\"",
          "publishCmd": "pnpm publish --access public --no-git-checks"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "volta": {
    "node": "22.7.0"
  }
}
