{
  "name": "prisma-generator-plantuml-erd",
  "description": "Provide a description about your generator",
  "version": "1.3.1",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-plantuml-erd": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "@prisma/generator-helper": "3.12.0",
    "zod": "3.19.1"
  },
  "peerDependencies": {
    "@prisma/client": "3.12.0",
    "@prisma/generator-helper": "3.12.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@prisma/sdk": "^3.15.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.2.2",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.2",
    "jest": "^29.3.0",
    "prettier": "^2.7.1",
    "prisma": "^3.12.0",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "typescript": "4.6.2"
  },
  "homepage": "https://github.com/dbgso/prisma-generator-plantuml-erd/tree/main/packages/generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbgso/prisma-generator-plantuml-erd.git"
  },
  "author": "osaki.shin <osaki.shin@dbgso.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "plantuml"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
